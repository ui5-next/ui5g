{
  "name": "<%= namespace %>",
  "displayName": "<%= name %>",
  "productName": "<%= name %>",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run lint:fix",
    "start:debug": "node_modules/.bin/gulp dev",
    "start:silent": "node_modules/.bin/gulp dev:silent",
    "start": "node_modules/.bin/gulp dev:preload",
    "build": "node_modules/.bin/gulp build:prod",
    "build:sourcemap": "node_modules/.bin/gulp build:preload",
    "lint:fix": "node_modules/.bin/eslint --fix \"src/**/*.js\""
  },
  "ui5": {
    "namespace": "<%= namespace %>",
    "build": {
      "resource": "https://<%= ui5Domain %>/<%= version %>/resources/",
      "src": "./src",
      "dist": "./dist",
      "offline": false
    }
  },
  "dependencies": {
    "lodash": "^4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@ui5-next/types": "latest",
    "babel-eslint": "^10",
    "babel-preset-ui5-next": "^7.4.3",
    "browser-sync": "2.26.7",
    "del": "^5",
    "eslint": "^6",
    "eslint-plugin-react": "^7",
    "gulp": "^4",
    "gulp-babel": "^8",
    "gulp-eslint": "^5.0.0",
    "gulp-filter": "^5.0.0",
    "gulp-less": "^4.0.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-ui5-eager-preload": "^1.19.5",
    "http-proxy-middleware": "^0.20.0",
    "merge-stream": "^1"
  }
}