{
  "app": {
    "namespace": "<%= namespace %>",
    "resource": "https://<%= ui5Domain %>/resources/"
  },
  "name": "<%= name %>",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run lint:fix",
    "start:debug": "node_modules/.bin/gulp dev",
    "start": "node_modules/.bin/gulp dev:preload",
    "build": "node_modules/.bin/gulp build",
    "build:sourcemap": "node_modules/.bin/gulp build:sourcemap",
    "lint:fix": "node_modules/.bin/eslint --fix \"src/**/*.js\""
  },
  "dependencies": {
    "lodash": "^4"
  },
  "devDependencies": {
    "@types/openui5": "^1.40.2",
    "@ui5-next/types": "latest",
    "babel-cli": "^6",
    "babel-core": "^6",
    "babel-eslint": "^10",
    "babel-preset-flow": "^6",
    "babel-preset-ui5-next": "^1.6.3",
    "browser-sync": "2.26.7",
    "del": "^5",
    "eslint": "^6",
    "eslint-plugin-react": "^7",
    "gulp": "^4",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^5.0.0",
    "gulp-filter": "^5.0.0",
    "gulp-less": "^4.0.1",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-ui5-eager-preload": "^1.12.20",
    "http-proxy-middleware": "^0.19.1",
    "merge-stream": "^1.0.1"
  }
}
