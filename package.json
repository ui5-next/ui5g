{
  "name": "generator-ui5g",
  "version": "1.20.1",
  "description": "yet another generator for sapui5/openui5",
  "author": {
    "name": "Theo Sun",
    "email": "theo.sun@sap.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "ui5"
  ],
  "scripts": {
    "release": "standard-version --commit-all --no-verify && git push --follow-tags origin master"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^11.0.0",
    "eslint": "^7.9.0",
    "eslint-config-xo-space": "^0.25.0",
    "husky": "^4.3.0",
    "standard-version": "^9.0.0",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^3.0.0"
  },
  "dependencies": {
    "@newdash/newdash": "^5.13.0",
    "chalk": "^4.1.0",
    "mkdirp": "^1.0.4",
    "node-fetch": "^2.6.1",
    "yeoman-generator": "^4.12.0",
    "yosay": "^2.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "jest": true,
      "node": true
    }
  },
  "repository": "https://github.com/Soontao/ui5g",
  "license": "MIT"
